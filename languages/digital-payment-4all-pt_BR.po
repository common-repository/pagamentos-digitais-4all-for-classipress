msgstr "O CPF do comprador é um campo obrigatório"

#: includes/class-digital-payment-4all.php:82
msgid "Invalid buyer document"
msgstr "Documento do comprador inválido"

#: includes/class-digital-payment-4all.php:86
msgid "Expiration date is a required field"
msgstr "A data de expiração é um campo obrigatório"

#: includes/class-digital-payment-4all.php:88
msgid "Invalid expiration date"
msgstr "Data de expiração inválida"

#: includes/class-digital-payment-4all.php:92
msgid "Security code is a required field"
msgstr "Código de segurança é um campo obrigatório"

#: includes/class-digital-payment-4all.php:94
msgid "Invalid security code"
msgstr "Código de segurança inválido"

